n  transition: all 0.5s ease-in-out;\r\n}\r\n\r\nheader {\r\n  display: flex;\r\n  flex-direction: column;\r\n  background-image: url('../assets/stars.jpg');\r\n  background-size: cover;\r\n  background-position: bottom;\r\n  background-repeat: no-repeat;\r\n  min-height: 15vh;\r\n\r\n  font-family: 'Times New Roman', Times, serif;\r\n\r\n  font-size: clamp(1.5rem, 5vw, 3rem);\r\n  text-transform: uppercase;\r\n  position: relative;\r\n}\r\n\r\nheader ul {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  width: 100%;\r\n  height: auto;\r\n  position: fixed;\r\n}\r\n\r\nheader ul li {\r\n  background-image: linear-gradient(\r\n    90deg,\r\n    rgb(60, 60, 64) 0%,\r\n    rgb(231, 21, 21) 0%,\r\n    rgba(0, 212, 255, 1) 100%\r\n  );\r\n  background-size: cover;\r\n  background-clip: text;\r\n\r\n  -webkit-background-clip: text;\r\n  -webkit-text-fill-color: transparent;\r\n  font-size: clamp(1rem, 3vw, 2rem);\r\n  font-weight: 700;\r\n\r\n  letter-spacing: 0.1em;\r\n  transition: all 0.5s ease-in-out;\r\n}\r\n\r\n.mask {\r\n  position: absolute;\r\n  inset: 0;\r\n\r\n  -webkit-mask-image: url('../assets/rect.png');\r\n  -webkit-mask-size: cover;\r\n  -webkit-mask-position: bottom;\r\n  mask-image: url('../assets/rect.png');\r\n  mask-size: cover;\r\n  mask-position: bottom;\r\n}\r\n\r\n#id {\r\n  display: none;\r\n}\r\n.hide {\r\n  background-color: burlywood;\r\n}\r\n\r\nbody {\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 100%;\r\n}\r\n\r\n.list-item::marker {\r\n  color: blue;\r\n  content: '✝ ';\r\n  font-size: 1.2em;\r\n}\r\n\r\nform {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  margin: 3rem;\r\n  padding: 1rem;\r\n  background-color: lightblue;\r\n}\r\n\r\nform .email {\r\n  color: blueviolet;\r\n}\r\n\r\nform .address {\r\n  color: peru;\r\n}\r\n\r\nform .name {\r\n  color: orangered;\r\n}\r\n\r\nfooter {\r\n  position: absolute;\r\n  bottom: 0;\r\